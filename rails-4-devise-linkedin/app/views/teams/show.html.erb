<h1>Team</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
    	<th class="col-xs-3">Id</th>
      	<th class="col-xs-6">Name</th>
      	<th class="col-xs-3">Client Id</th>
    </tr>
  </thead>

  <tbody>
      <tr>
      	<td><%= @team.id %></td>
        <td><%= @team.name %></td>
        <td><%= @team.client_id %></td>
      </tr>
  </tbody>
</table>

<%= link_to 'Back', teams_path ,class: 'btn btn-info'%>
<%= link_to 'Edit', edit_team_path(@team) ,class: 'btn btn-primary'%>
